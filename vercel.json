{
    "version": 2,
    "functions": {
        "api/index.py": {
            "maxDuration": 10,
            "includeFiles": [
                "models/**",
                "templates/**",
                "static/**"
            ]
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ]
}